
{{> theme_cyber_range/partials/head }}

<body id="dashboard">
{{> theme_cyber_range/navbar }}
{{> core/local/toast/wrapper}}

<div class="height-100 bg-light mt-3">
  <div class="wrapper">
    {{> theme_cyber_range/partials/search }}

    <div class="container mb-3">
    <div class="onboarding">
        <section class="row">
        <div class="col-3 stage">User Profile <ekc-icon class="complete" type="check-circle"></ekc-icon></div>
        <div class="col-3 stage">Skill Assessment</div>
        <div class="col-3 stage">Select Modules</div>
        <div class="col-3 stage">Tutorial Materials</div>
        </section>
        <section class="progress-wrapper">
        <div class="progress mb-1 ">
            <div class="progress-bar bg-success w-25" role="progressbar" aria-valuenow="25" aria-valuemin="0"
            aria-valuemax="100"></div>
        </div>
        </section>
    </div>
    </div>
    <div class="container">
        
    </div>
  </div>
</div>

{{> theme_cyber_range/partials/footer }}
</body>
</html>
{{#js}}
M.util.js_pending('theme_boost/loader');
require(['theme_boost/loader'], function() {
    M.util.js_complete('theme_boost/loader');
});

M.util.js_pending('theme_boost/drawer');
require(['theme_boost/drawer'], function(drawer) {
    drawer.init();
    M.util.js_complete('theme_boost/drawer');
});
{{/js}}
